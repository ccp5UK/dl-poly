# General Rules

.c.o:
	$(CC) $(CFLAGS) -o $(VPATH)/$@ -c $<


.f.o:
	$(F77) $(FFLAGS) -o $(VPATH)/$@ -c $<

default:    codegen lib bin


# File-specific Rules

$(PALOUT): $(PAL)
	$(PALPROC) $(PAL) $(PALPROCFLAGS)

