# Author - I.T.Todorov January 2015
#
#
# Define default settings
#=====================================================================

SHELL=/bin/sh

.SUFFIXES:
.SUFFIXES: .f90 .F90 .o

BINROOT=../execute
EX=DLPOLY.Z
EXE=$(BINROOT)/$(EX)

TYPE=master

FC=undefined
LD=undefined

